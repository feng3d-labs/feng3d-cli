{
  "name": "feng3d-cli",
  "version": "0.0.3",
  "description": "feng3d 项目通用标准，包含代码规范、配置模板和 CLI 工具",
  "homepage": "https://feng3d.com/feng3d-cli/",
  "author": "feng",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "feng3d-cli": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./eslint": {
      "types": "./dist/eslint.d.ts",
      "import": "./dist/eslint.js"
    }
  },
  "scripts": {
    "clean": "rimraf \"{lib,dist,public}\"",
    "build": "tsc",
    "dev": "tsc -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "types": "tsc",
    "watch": "tsc -w",
    "lint": "eslint . --ext .js,.ts --max-warnings 0",
    "lintfix": "npm run lint -- --fix",
    "docs": "typedoc",
    "upload_oss": "npm run build && npm run docs && node dist/cli.js oss_upload_dir",
    "update": "npm run build && node dist/cli.js update && npm install",
    "release": "npm run clean && npm run lint && npm run build && npm run docs && npm publish",
    "prepublishOnly": "npm run lint && npm test && npm run build",
    "publish:manual": "npm publish",
    "prepare": "husky"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/feng3d-labs/feng3d-cli.git"
  },
  "files": [
    "dist",
    "templates",
    "schemas"
  ],
  "dependencies": {
    "ali-oss": "^6.22.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "eslint": "9.26.0",
    "globals": "^14.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "rimraf": "6.0.1",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.4",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix --max-warnings 0"
    ]
  }
}
